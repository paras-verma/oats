{
  "name": "oats",
  "version": "1.0.0",
  "description": "Generate Express (Typescript) server stubs from OAS3.0",
  "author": "maaspiar",
  "license": "GNU3.0",
  "type": "module",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "bin": {
    "oats": "./dist/index.js"
  },
  "engines": {
    "node": ">=14.16"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@swc/core": "^1.3.60",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.1.4",
    "js-yaml": "^4.1.0",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/mustache": "^4.2.2",
    "@types/node": "^18.15.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "prepare": "npx @openapitools/openapi-generator-cli version-manager set 5.2.0",
    "prebuild": "rm -rf dist",
    "build": "tsup",
    "postbuild": "cp -r cli/assets/ dist/assets/",
    "predev": "npm run prebuild",
    "dev": "tsup --watch"
  },
  "keywords": [
    "typescript",
    "codegen",
    "express",
    "stub",
    "oas",
    "openapi"
  ]
}
