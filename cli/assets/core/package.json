{
  "name": "express-ts-server",
  "version": "0.0.0",
  "description": "Express Server in Typescript | Generate by OATS",
  "scripts": {
    "dev": "nodemon --watch . -e ts,tsx --exec 'npm run build -- dev && npm run host'",
    "host": "node ./build-local/index.js",
    "predeploy": "npm run build -- prod",
    "build": "./build.sh",
    "deploy": "./deploy.sh"
  },
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-openapi-validator": "^5.0.4",
    "mongoose": "^6.11.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/node": "^18.7.18",
    "nodemon": "^2.0.19",
    "typescript": "^4.8.3"
  }
}
